<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.transporterapp.syde.transporterapp.History.HistOverviewMonthly">

    <TableLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:stretchColumns="*" android:id="@+id/monthlyTable">
        <TableRow>
            <TextView
                android:id="@+id/dateHeader"
                android:layout_width="0dp"
                android:layout_weight="5.5"
                android:text="Date"
                android:textSize="14dp"
                android:textColor="@color/bodyText"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_margin="@dimen/text_margin"
                android:textAppearance="?attr/textAppearanceListItem" />

            <TextView
                android:layout_width="0dp"
                android:layout_weight="3"
                android:layout_height="match_parent"
                android:text="Number of Collections"
                android:textSize="10dp"
                android:textColor="@color/bodyText"
                android:gravity="center"
                android:id="@+id/numberOfCollectionsHeader"/>

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:id="@+id/volumeHeader"
                android:textColor="@color/bodyText"
                android:text="Volume"
                android:textSize="14dp"
                android:gravity="center"
                android:layout_margin="@dimen/text_margin"
                android:textAppearance="?attr/textAppearanceListItem"/>

            <RelativeLayout>

            </RelativeLayout>
        </TableRow>
    </TableLayout>

</FrameLayout>
